  "bugs": {
    "url": "https://github.com/ascoders/typescript-template/issues"
  },
  "homepage": "https://github.com/ascoders/typescript-template#readme",
  "devDependencies": {
    "@types/node": "*",
    "@types/react": "*",
    "@types/webpack": "*",
    "ascoders-tslint-config": "^1.0.2",
    "ava": "^0.23.0",
    "codecov": "^3.0.0",
    "nyc": "^11.3.0",
    "pre-commit": "^1.2.2",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "run-react": "^2.0.6",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1"
  },
  "dependencies": {},
